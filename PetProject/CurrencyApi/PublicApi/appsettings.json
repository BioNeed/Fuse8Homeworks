{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Grpc": "Debug"
    }
  },
  "ConnectionStrings": {
    "SummerSchool": "Host=localhost;Port=5432;Database=SummerSchool;Username=postgres;Password=1234;",
    "DockerSummerSchool": "Host=localhost;Port=5434;Database=SummerSchool;Username=root;Password=root"
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Error",
        "Microsoft": "Error",
        "Microsoft.Hosting": "Information",
        "Grpc": "Debug"
      }
    },
    "WriteTo:Console": {
      "Name": "Console",
      "Args": {
        "outputTemplate": "[GRPC]{NewLine}{Timestamp:dd.MM.yy HH.mm.ss.fff} [{Level:u3}]{NewLine}{Message:lj} {Exception}{NewLine}"
      }
    }
  },
  "ApiSettings": {
    "BaseAddress": "http://localhost:5236"
  }
}
