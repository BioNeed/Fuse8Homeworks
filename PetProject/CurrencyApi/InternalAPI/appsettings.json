{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Error",
        "Microsoft": "Error",
        "Microsoft.Hosting": "Information"
      }
    },
    "WriteTo:Console": {
      "Name": "Console",
      "Args": {
        "outputTemplate": "{Timestamp:dd.MM.yy HH.mm.ss.fff} [{Level:u3}]{NewLine}{Message:lj} {Exception}{NewLine}"
      }
    }
  },
  "Settings": {
    "DefaultCurrency": "RUB",
    "BaseCurrency": "USD",
    "CurrencyRoundCount": 2
  },
  "ApiSettings": {
    "BaseAddress": "https://api.currencyapi.com/",
    "CacheExpirationTimeInHours":  "2"
  }
}
